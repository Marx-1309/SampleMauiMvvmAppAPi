{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "WaterBillingMobileAppAPiContext": "Data Source=localhost;Initial Catalog=OTCWB_Db;Integrated Security=True"
  },
  "JwtSettings": {
    "Issuer": "WaterBillingMobileAppAPi",
    "Audience": "MauiApp",
    "DurationInMintues": 1,
    "Key": "YourSecretEncodingKey"
  }
}